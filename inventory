[local]
workstation ansible_host=localhost ansible_connection=local ansible_user=ansible ansible_become=yes

[openstack_config]
director ansible_host=192.168.250.10 ansible_user=cloud-user ansible_password=redhat ansible_become=yes ansible_ssh_extra_args='-o StrictHostKeyChecking=no'

[openstack_oper]
undercloud ansible_host=192.168.250.10 ansible_user=stack ansible_become=yes ansible_ssh_extra_args='-o StrictHostKeyChecking=no'
